{
  "manifest_version": 3,
  "name": "VKMusic Plugin",
  "version": "2.1",
  "description": "VKMusic Plugin openBETA",
  "permissions": ["scripting","storage"],
  "content_scripts": [
    {
      "matches": ["*://vk.com/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "inject.js" ],
      "matches": [ "https://*/*" ]
    }
  ]
}

